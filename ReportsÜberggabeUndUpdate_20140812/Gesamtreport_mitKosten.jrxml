(select obmedium from objekt where obid = id_des_elternobjektes) as medienid_des_elternobjektes,
(select mename from media where meid = medienid_des_elternobjektes and meid!=29) as name_des_mediums_des_elternobjektes,
(select mekosten from media where meid = medienid_des_elternobjektes) as kosten_des_mediums_des_elternobjektes,
(select get_diff($P{zeitstempel},obid, $P{zeitmodus}, 1)) as Realverbrauch,
(select get_diff($P{zeitstempel},obid, $P{zeitmodus}, 1)*obArbeitsaequivalent) as Verbrauch,
(select get_timestamp($P{zeitstempel}, $P{zeitmodus}, 1)) as Zeitstempel,
(select Realverbrauch*kosten_des_mediums_des_elternobjektes) as Kosten,
(select obName from objekt where obid=$P{obid}) as Bereich,
(select mename from media where meid = $P{medium}) as Mediumname1,
(select mename from media where meid = $P{medium1}) as Mediumname2,
(select mename from media where meid = $P{medium2}) as Mediumname3
FROM objekt, unit
where obart=3
and
obDatenbasis != 3
and
get_tree(obid) like concat('%$', $P{obid},'$%') and obEinheitZ = unId]]>
